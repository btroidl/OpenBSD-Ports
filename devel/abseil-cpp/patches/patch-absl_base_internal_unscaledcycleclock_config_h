-    defined(__powerpc__) || defined(__ppc__) || defined(__riscv) ||     \
+ /* defined(__powerpc__) || defined(__ppc__) || */ defined(__riscv) ||     \
     defined(_M_IX86) || (defined(_M_X64) && !defined(_M_ARM64EC))
 #define ABSL_HAVE_UNSCALED_CYCLECLOCK_IMPLEMENTATION 1
